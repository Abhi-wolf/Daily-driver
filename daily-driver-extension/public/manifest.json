{
  "manifest_version": 3,
  "name": "daily-driver",
  "icons": {
    "32": "icon1.png"
  },
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "scripting", "storage", "alarms", "notifications"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
